{
	"user": "Cezear",
	"repo": "Regular-Expressions-Plugin",
	"resources": [ 
		{
			"name": "regex-(.*)-win.rar",
			"platform": "windows",
			"archive": true,
			"includes": ["pawno/include"],
			"plugins": ["plugins/libRegEx.dll"],
			"files": {
				"onig.dll": "onig.dll"
			}
		},
		{
			"name": "regex-(.*)-linux.tar.gz$",
			"platform": "linux",
			"archive": true,
			"plugins": ["plugins/libRegEx_static.so"]
		}
	],
	"runtime": {
		"plugins": ["Cezear/Regular-Expressions-Plugin"]
	}
}